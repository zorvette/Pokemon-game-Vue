{"ast":null,"code":"import pokemonPicture from '@/components/pokemosPicture.vue';\nimport pokemonOptions from '@/components/pokemonOptions.vue';\nimport getPokemonOptions from '@/helpers/getPokemonOptions'; // console.log( getPokemonOptions() )\n\nexport default {\n  components: {\n    pokemonPicture,\n    pokemonOptions\n  },\n\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      message: \"\"\n    };\n  },\n\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      const rndInt = Math.floor(Math.random() * 4);\n      this.pokemon = this.pokemonArr[rndInt];\n    },\n\n    checkAnswer(pokemonId) {\n      this.showPokemon = true;\n    }\n\n  },\n\n  mounted() {\n    this.mixPokemonArray();\n  }\n\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AAEA,OAAOC,iBAAP,MAA8B,6BAA9B,C,CAEA;;AAEA,eAAe;EACbC,UAAU,EAAE;IAAEH,cAAF;IAAkBC;EAAlB,CADC;;EAEbG,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE,EADP;MAELC,OAAO,EAAE,IAFJ;MAGLC,WAAW,EAAE,KAHR;MAILC,OAAO,EAAE;IAJJ,CAAP;EAMD,CATY;;EAUbC,OAAO,EAAE;IACP,MAAMC,eAAN,GAAwB;MACtB,KAAKL,UAAL,GAAkB,MAAMH,iBAAiB,EAAzC;MAEA,MAAMS,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAYD,IAAI,CAACE,MAAL,KAAgB,CAA5B,CAAf;MAEF,KAAKR,OAAL,GAAe,KAAKD,UAAL,CAAiBM,MAAjB,CAAf;IACC,CAPM;;IAQPI,WAAW,CAAEC,SAAF,EAAc;MACvB,KAAKT,WAAL,GAAmB,IAAnB;IACF;;EAVO,CAVI;;EAsBbU,OAAO,GAAG;IACR,KAAKP,eAAL;EACF;;AAxBa,CAAf","names":["pokemonPicture","pokemonOptions","getPokemonOptions","components","data","pokemonArr","pokemon","showPokemon","message","methods","mixPokemonArray","rndInt","Math","floor","random","checkAnswer","pokemonId","mounted"],"sourceRoot":"","sources":["/home/ko/Documentos/cursos/Vue/04-pokemon-game/src/pages/pokemonPages.vue"],"sourcesContent":["<template>\n\n<h1 v-if=\"!pokemon\" >Espere por favor...</h1>\n\n  <div v-else>\n  <h1>¿Quién es este pokémon?</h1>\n\n  <pokemonPicture \n    :pokemonId=\"pokemon.id\" \n    :showPokemon=\"showPokemon\"\n  />\n  <pokemonOptions \n    :pokemons=\"pokemonArr\"\n    @selection=\"checkAnswer\"\n  />\n\n  <h2> {{message}} </h2>\n  <button>\n    Nuevo Juego\n  </button>\n  </div>\n\n</template>\n\n<script>\nimport pokemonPicture from '@/components/pokemosPicture.vue'\nimport pokemonOptions from '@/components/pokemonOptions.vue'\n\nimport getPokemonOptions from '@/helpers/getPokemonOptions'\n\n// console.log( getPokemonOptions() )\n\nexport default {\n  components: { pokemonPicture, pokemonOptions },\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      message: \"\"\n    }\n  },\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions()\n\n      const rndInt = Math.floor( Math.random() * 4 )\n\n    this.pokemon = this.pokemonArr[ rndInt ]\n    },\n    checkAnswer( pokemonId ) {\n      this.showPokemon = true\n    }\n  },\n  mounted() {\n    this.mixPokemonArray()\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}