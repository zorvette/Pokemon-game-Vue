{"ast":null,"code":"import pokemonPicture from '@/components/pokemosPicture.vue';\nimport pokemonOptions from '@/components/pokemonOptions.vue';\nimport getPokemonOptions from '@/helpers/getPokemonOptions'; // console.log( getPokemonOptions() )\n\nexport default {\n  components: {\n    pokemonPicture,\n    pokemonOptions\n  },\n\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      showAnswer: false,\n      message: \"\"\n    };\n  },\n\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      const rndInt = Math.floor(Math.random() * 4);\n      this.pokemon = this.pokemonArr[rndInt];\n    },\n\n    checkAnswer(selectedId) {\n      this.showPokemon = true;\n      this.showAnswer = true;\n\n      if (selectedId === this.pokemon.id) {\n        this.message = `Correcto, ${this.pokemon.name}`;\n      } else {\n        this.message = `Oops, era ${this.pokemon.name}`;\n      }\n    },\n\n    newGame() {\n      this.showPokemon = false;\n      this.showAnswer = false;\n      this.pokemon = null;\n      this.pokemonArr = [];\n      this.mixPokemonArray();\n    }\n\n  },\n\n  mounted() {\n    this.mixPokemonArray();\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AAEA,OAAOC,iBAAP,MAA8B,6BAA9B,C,CAEA;;AAEA,eAAe;EACbC,UAAU,EAAE;IAAEH,cAAF;IAAkBC;EAAlB,CADC;;EAEbG,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE,EADP;MAELC,OAAO,EAAE,IAFJ;MAGLC,WAAW,EAAE,KAHR;MAILC,UAAU,EAAE,KAJP;MAKLC,OAAO,EAAE;IALJ,CAAP;EAOD,CAVY;;EAWbC,OAAO,EAAE;IACP,MAAMC,eAAN,GAAwB;MACtB,KAAKN,UAAL,GAAkB,MAAMH,iBAAiB,EAAzC;MAEA,MAAMU,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAYD,IAAI,CAACE,MAAL,KAAgB,CAA5B,CAAf;MAEF,KAAKT,OAAL,GAAe,KAAKD,UAAL,CAAiBO,MAAjB,CAAf;IACC,CAPM;;IAQPI,WAAW,CAAEC,UAAF,EAAe;MACxB,KAAKV,WAAL,GAAmB,IAAnB;MACA,KAAKC,UAAL,GAAkB,IAAlB;;MAEA,IAAIS,UAAS,KAAM,KAAKX,OAAL,CAAaY,EAAhC,EAAqC;QACnC,KAAKT,OAAL,GAAgB,aAAY,KAAKH,OAAL,CAAaa,IAAK,EAA9C;MACF,CAFA,MAEO;QACL,KAAKV,OAAL,GAAgB,aAAY,KAAKH,OAAL,CAAaa,IAAK,EAA9C;MACF;IACD,CAjBM;;IAkBPC,OAAO,GAAG;MACR,KAAKb,WAAL,GAAmB,KAAnB;MACA,KAAKC,UAAL,GAAmB,KAAnB;MACA,KAAKF,OAAL,GAAmB,IAAnB;MACA,KAAKD,UAAL,GAAmB,EAAnB;MACA,KAAKM,eAAL;IACF;;EAxBO,CAXI;;EAqCbU,OAAO,GAAG;IACR,KAAKV,eAAL;EACF;;AAvCa,CAAf","names":["pokemonPicture","pokemonOptions","getPokemonOptions","components","data","pokemonArr","pokemon","showPokemon","showAnswer","message","methods","mixPokemonArray","rndInt","Math","floor","random","checkAnswer","selectedId","id","name","newGame","mounted"],"sourceRoot":"","sources":["/home/ko/Documentos/cursos/Vue/04-pokemon-game/src/pages/pokemonPages.vue"],"sourcesContent":["<template>\n\n<h1 v-if=\"!pokemon\" >Espere por favor...</h1>\n\n  <div v-else>\n  <h1>¿Quién es este pokémon?</h1>\n\n  <pokemonPicture \n    :pokemonId=\"pokemon.id\" \n    :showPokemon=\"showPokemon\"\n  />\n  <pokemonOptions \n    :pokemons=\"pokemonArr\"\n    @selection=\"checkAnswer\"\n  />\n\n    <div v-if=\"showAnswer\">\n      <h2 class=\"fade-in\"> {{message}} </h2>\n      <button @click=\"newGame\" >\n        Nuevo Juego\n      </button>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport pokemonPicture from '@/components/pokemosPicture.vue'\nimport pokemonOptions from '@/components/pokemonOptions.vue'\n\nimport getPokemonOptions from '@/helpers/getPokemonOptions'\n\n// console.log( getPokemonOptions() )\n\nexport default {\n  components: { pokemonPicture, pokemonOptions },\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      showAnswer: false,\n      message: \"\"\n    }\n  },\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions()\n\n      const rndInt = Math.floor( Math.random() * 4 )\n\n    this.pokemon = this.pokemonArr[ rndInt ]\n    },\n    checkAnswer( selectedId ) {\n      this.showPokemon = true\n      this.showAnswer = true\n\n      if( selectedId === this.pokemon.id ) {\n        this.message = `Correcto, ${this.pokemon.name}`\n      } else {\n        this.message = `Oops, era ${this.pokemon.name}`\n      }\n    },\n    newGame() {\n      this.showPokemon = false\n      this.showAnswer  = false\n      this.pokemon     = null\n      this.pokemonArr  = []\n      this.mixPokemonArray()\n    }\n  },\n  mounted() {\n    this.mixPokemonArray()\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}